include_rules

CPPFLAGS += -fPIC -I ../src -I/usr/include/python2.7

LDFLAGS += -shared -lpython2.7 -lncurses

!htmllinker = |> ^ HTML LD %o^ $(LD) $(LDFLAGS) $(SHARED_LIBS) -L /usr/local/lib -lboost_python -o %o %f |> pytungsten.so

: foreach *.cpp |> !cxx |>
: *.o ../src/*.o |> !htmllinker |>
